{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 300,
            "memory": 1024
        }
    }
}